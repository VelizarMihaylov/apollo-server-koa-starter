{
    "extends": [
      "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "jest",
      "@typescript-eslint"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    },
    "env": {
      "jest/globals": true
    }
}
